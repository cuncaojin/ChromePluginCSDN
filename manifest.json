{
  "name": "COPY CSDN",
  "description": "cuncaojin: can copy csdn code.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "/js/background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["*://*.csdn.net/*"],
  "optional_host_permissions": ["*://*/*"],
  "action": {
    "default_popup": "/html/popup.html",
    "default_icon": {
      "16": "/image/get_started16.png",
      "32": "/image/get_started32.png",
      "48": "/image/get_started48.png",
      "128": "/image/get_started128.png"
    }
  },
  "icons": {
    "16": "/image/get_started16.png",
    "32": "/image/get_started32.png",
    "48": "/image/get_started48.png",
    "128": "/image/get_started128.png"
  },
  "options_page": "/html/options.html",
  "content_scripts": [
    {
      "matches": ["*://*.csdn.net/*", "*://*/*"],
      "exclude_matches": ["*://*/*business*"],
      "all_frames": true,
      "js": ["/js/content.js"],
      "css": ["css/content.css", "css/toast.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["image/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
